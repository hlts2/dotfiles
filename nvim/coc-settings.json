{
  "signature": {
    "preferShownAbove": false,
    "target": "float"
  },
  "session.directory": "~/.cache/vim/sessions",
  "diagnostic": {
    "messageTarget": "echo",
    "refreshOnInsertMode": true,
      "infoSign": "‚ÑπÔ∏è",
      "hintSign": "üí°",
      "warningSign": "‚ö†Ô∏è",
      "errorSign": "‚ùå"
  },
  "suggest": {
    "detailField": "abbr",
    "echodocSupport": true,
    "enablePreselect": false,
    "enablePreview": true,
    "floatEnable": true,
    "keepCompleteopt": false,
    "noselect": true,
    "preferCompleteThanJumpPlaceholder": true,
    "removeDuplicateItems": true,
    "snippetIndicator": "",
    "triggerAfterInsertEnter": true
  },
  "languageserver": {
      "cue": {
          "command": "cuelsp",
          "filetypes": ["cue"],
          "rootPatterns": [
              "cue.mod"
          ],
          "initializationOptions": {
              "cacheDirectory": "/tmp/cue",
              "highlight": {
                  "enabled": true
              }
          }
      }
  },
  "sumneko-lua.enableNvimLuaDev": true,
  "go.goplsEnv": {
      "GOFLAGS": "-tags=e2e"
  },
  "go.goplsPath": "/home/hlts2.linux/go/bin/gopls",
  "Lua.workspace.checkThirdParty": false
}


