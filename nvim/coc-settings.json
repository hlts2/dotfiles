{
    "signature": {
        "preferShownAbove": true
    },
    "session.directory": "~/.cache/vim/sessions",
    "diagnostic": {
        "checkCurrentLine": true,
        "highlightLimit": 5000,
        "messageTarget": "echo",
        "refreshOnInsertMode": true
    },
    "suggest": {
        "defaultSortMethod": "alphabetical",
        "enablePreselect": false,
        "noselect": true,
        "preferCompleteThanJumpPlaceholder": true,
        "removeDuplicateItems": true,
        "snippetIndicator": "~",
        "timeout": 8000,
        "triggerAfterInsertEnter": true
    },
    "languageserver": {
        "cue": {
            "command": "cuelsp",
            "filetypes": ["cue"],
            "rootPatterns": [
                "cue.mod"
            ],
            "initializationOptions": {
                "cacheDirectory": "/tmp/cue",
                "highlight": {
                    "enabled": true
                }
            }
        }
    },
    "sumneko-lua": {
        "enableNvimLuaDev": true
    },
    "go": {
        "goplsEnv": {
            "GOFLAGS": "-tags=e2e,mage"
        }
    },
    "Lua": {
        "workspace": {
            "checkThirdParty": false
        }
    }
}


/**

  "go.goplsPath": "/home/hlts2.linux/go/bin/gopls",
    "yaml.schemas": {
        "kubernetes": "/*.yaml"
    }
**/
